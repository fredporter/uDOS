{
  "$schema": "../container.schema.json",
  "container": {
    "id": "markdown-to-pdf",
    "name": "Markdown to PDF",
    "description": "Convert Markdown documents to PDF format",
    "type": "git",
    "source": "https://github.com/JayJay1024/markdown-to-pdf",
    "ref": "main",
    "cloned_at": null,
    "last_update": null,
    "commit": null,
    "version": null
  },
  "policy": {
    "read_only": true,
    "auto_update": false,
    "update_channel": "wizard_only",
    "verify_signatures": false
  },
  "integration": {
    "wrapper_path": "extensions/docs/md2pdf/",
    "handler_path": "core/commands/export_handler.py",
    "install_script": "extensions/setup/install_md2pdf.py"
  },
  "build": {
    "type": "npm",
    "build_command": "npm install",
    "runtime": "node",
    "tce_dependencies": [
      "nodejs",
      "chromium"
    ]
  },
  "metadata": {
    "license": "MIT",
    "homepage": "https://github.com/JayJay1024/markdown-to-pdf",
    "documentation": "extensions/docs/md2pdf/README.md",
    "maintainer": "uDOS Wizard Server",
    "category": "documentation"
  }
}
