{
  "$schema": "../container.schema.json",
  "container": {
    "id": "gemini-cli",
    "name": "Gemini CLI",
    "description": "Google Gemini AI assistant for terminal",
    "type": "git",
    "source": "https://github.com/google-gemini/gemini-cli",
    "ref": "main",
    "cloned_at": null,
    "last_update": null,
    "commit": null,
    "version": null
  },
  "policy": {
    "read_only": true,
    "auto_update": true,
    "update_channel": "wizard_only",
    "verify_signatures": false,
    "wizard_only": true
  },
  "integration": {
    "wrapper_path": "extensions/ai/gemini/",
    "service_path": null,
    "handler_path": "core/commands/ai_handler.py",
    "install_script": "extensions/setup/install_gemini.sh"
  },
  "metadata": {
    "license": "Apache-2.0",
    "homepage": "https://github.com/google-gemini/gemini-cli",
    "documentation": "extensions/ai/gemini/README.md",
    "maintainer": "uDOS Wizard Server",
    "category": "ai"
  },
  "build": {
    "type": "npm",
    "build_command": "npm install && npm run build",
    "runtime": "node",
    "tce_dependencies": [
      "nodejs"
    ]
  },
  "config": {
    "api_key_required": true,
    "api_key_env": "GEMINI_API_KEY",
    "rate_limited": true
  },
  "_note": "WIZARD ONLY - Requires API key and web access. Not distributed to user devices.",
  "repo_path": "memory/library/containers/gemini-cli"
}