{
  "profile": "default",
  "description": "Example HubSpot CRM integration key reference. API key is stored in secrets.tomb and resolved by key_id.",
  "api_key_id": "hubspot-crm-main",
  "workspace_name": "default",
  "endpoints": {
    "base": "https://api.hubapi.com",
    "contacts": "/crm/v3/objects/contacts",
    "companies": "/crm/v3/objects/companies",
    "deals": "/crm/v3/objects/deals"
  },
  "sync_config": {
    "direction": "both",
    "auto_sync_enabled": false,
    "sync_interval_hours": 24,
    "contact_fields": [
      "firstname",
      "lastname",
      "email",
      "phone",
      "company",
      "jobtitle",
      "lifecyclestage",
      "notes"
    ]
  }
}
